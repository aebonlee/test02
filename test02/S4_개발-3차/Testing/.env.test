# @task S4T2
# Test Environment Variables
# Jest 통합 테스트용 환경 변수

# Supabase Configuration (테스트용 - 실제 값으로 교체 필요)
SUPABASE_URL=https://test-project.supabase.co
SUPABASE_ANON_KEY=test-anon-key-replace-with-real-value
SUPABASE_SERVICE_ROLE_KEY=test-service-role-key-replace-with-real-value

# Site Configuration
SITE_URL=http://localhost:8888
VERCEL_URL=localhost:8888
NEXT_PUBLIC_SITE_URL=http://localhost:8888

# Toss Payments (테스트용)
TOSS_CLIENT_KEY=test_ck_test-key-replace-with-real-value
TOSS_SECRET_KEY=test_sk_test-key-replace-with-real-value

# Resend Email (테스트용)
RESEND_API_KEY=re_test-key-replace-with-real-value
RESEND_FROM_EMAIL=test@example.com

# AI Service Keys (테스트용)
GEMINI_API_KEY=test-gemini-key-replace-with-real-value
OPENAI_API_KEY=test-openai-key-replace-with-real-value
PERPLEXITY_API_KEY=test-perplexity-key-replace-with-real-value

# Google OAuth (테스트용)
GOOGLE_CLIENT_ID=test-client-id.apps.googleusercontent.com
GOOGLE_CLIENT_SECRET=test-client-secret

# Test Configuration
NODE_ENV=test
TEST_TIMEOUT=30000
ENABLE_API_MOCKING=true

# Rate Limiting (테스트 시 비활성화)
RATE_LIMIT_ENABLED=false
RATE_LIMIT_MAX_REQUESTS=1000
RATE_LIMIT_WINDOW_MS=60000

# Logging
LOG_LEVEL=error
ENABLE_DEBUG_LOGS=false

# Database
DB_POOL_MIN=1
DB_POOL_MAX=5

# Security
JWT_SECRET=test-jwt-secret-do-not-use-in-production
ENCRYPTION_KEY=test-encryption-key-do-not-use-in-production

# Feature Flags (테스트용)
ENABLE_SUBSCRIPTION=true
ENABLE_PAYMENT=true
ENABLE_AI_FEATURES=true
ENABLE_EMAIL=true

# Webhook Secrets (테스트용)
TOSS_WEBHOOK_SECRET=test-webhook-secret
STRIPE_WEBHOOK_SECRET=test-stripe-webhook-secret

# Cache
ENABLE_CACHE=false
CACHE_TTL=60

# CORS
CORS_ORIGIN=http://localhost:8888
CORS_CREDENTIALS=true
